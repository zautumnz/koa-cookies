    "node": ">=24.0.0"
  },
  "devDependencies": {
    "@types/koa": "3.0.1",
    "@types/node": "24.9.2",
    "koa": "3.1.1",
    "npm-run-all": "4.1.5",
    "sortpack": "3.0.0",
    "supertest": "7.1.4",
    "tap-spec": "5.0.0",
    "tape": "5.9.0",
    "ts-node": "10.9.2",
    "ts-standard": "12.0.2",
    "typescript": "5.9.3"
